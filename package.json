{
	"name": "gulp-vituum-template",
	"version": "1.3",
	"packageManager": "yarn@3.5.0",
	"type": "module",
	"private": true,
	"license": "MIT",
	"engineStrict": true,
	"scripts": {
		"dev": "gulp --nowebp",
		"build": "gulp build --nowebp",
		"buildWithWebP": "gulp build",
		"deployFTPHtml": "gulp deployHTML --nowebp --ftpHtml",
		"deployFTPHtmlWebP": "gulp deployHTML --ftpHtml",
		"deployFTPServer": "gulp deployHTML --nowebp --ftpServer",
		"deployFTPServerWebP": "deployHTML --ftpServer",
		"sprite": "gulp sprite",
		"fonts": "gulp fonts --rewrite",
		"_viteDev": "vite dev --host 0.0.0.0",
		"_viteBuild": "vituum build --mode=production"
	},
	"workspaces": [
		"src/*"
	],
	"engines": {
		"node": ">=16"
	},
	"browserslist": [
		">1%",
		"not op_mini all",
		"not ie 11",
		"last 3 chrome version",
		"last 3 firefox version",
		"last 3 safari version"
	],
